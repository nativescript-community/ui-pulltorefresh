{
  "name": "@nativescript-community/ui-pulltorefresh",
  "version": "2.5.3",
  "description": "A NativeScript plugin to provide the Pull to Refresh control on any view.",
  "main": "index",
  "typings": "index.d.ts",
  "sideEffects": false,
  "nativescript": {
    "platforms": {
      "ios": "3.0.1",
      "android": "3.0.1"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "scripts": {
      "tsc": "cpy ../../src/pulltorefresh/index.d.ts ./ && tsc -d",
      "readme": "readme generate -c ../../tools/readme/blueprint.json",
      "build": "npm run tsc",
      "build.watch": "npm run tsc -- -w",
      "build.win": "npm run tsc-win",
      "build.all": "npm run build",
      "build.angular": "ng-packagr -p ../../src/pulltorefresh/angular/ng-package.json -c ../../src/pulltorefresh/angular/tsconfig.json && rm angular/.npmignore",
      "clean": "rimraf ./*.d.ts ./*.js ./*.js.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativescript-community/ui-pulltorefresh"
  },
  "keywords": [
    "NativeScript",
    "native",
    "script",
    "Pull Refresh",
    "SwipeRefreshLayout",
    "Material Design",
    "UIRefreshControl",
    "android",
    "ios",
    "bradmartin"
  ],
  "author": {
    "name": "Brad Martin",
    "email": "bradwaynemartin@gmail.com",
    "url": "https://github.com/bradmartin"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": "https://github.com/nativescript-community/ui-pulltorefresh/issues",
  "homepage": "https://github.com/nativescript-community/ui-pulltorefresh"
}
